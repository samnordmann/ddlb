{
    "benchmark": {
        "primitive": "tp_columnwise",
        "m": [512, 1024, 2048, 8192, 16384],
        "n": [512, 1024, 2048, 8192, 16384],
        "k": [512, 1024, 2048, 8192, 16384],
        "dtype": "float16",
        "validate": true,
        "num_iterations": 50,
        "num_warmups": 5,
        "time_measurement_backend": "cpu_clock",
        "barrier_at_each_iteration": true,
        "output_csv": "results/tp_columnwise_results_{timestamp}.csv",
        "implementations": {
            "compute_only": [
                {
                    "size": ["sharded", "unsharded"]
                }
            ],
            "pytorch": [
                {}
            ],
            "fuser": [
                {
                    "algorithm": ["default"],
                    "backend": ["nccl"]
                },
                {
                    "algorithm": ["default"],
                    "backend": ["cuda"],
                    "multicast_protocol": ["memcpy", "multimem"]
                },
                {
                    "algorithm": ["p2p_pipeline"],
                    "backend": ["nccl"]
                },
                {
                    "algorithm": ["p2p_pipeline"],
                    "backend": ["cuda"],
                    "inter_stream_synchronization": [true, false],
                    "offset_stream_indexing_by_rank": [true],
                    "multicast_protocol": ["memcpy", "multimem"]
                }
            ],
            "transformer_engine": [
                {}
            ],
            "jax": [
                {}
            ],
            "pytorch_sym_mem": [
                {}
            ],
            "ddlp": [
                {
                    "backend": ["pytorch", "fuser", "transformer_engine"]
                }
            ]
        }
    }
} 
